{
  "name": "express-app-example",
  "version": "1.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx --watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "docker:build": "docker build -t express-app-example .",
    "docker:run": "docker run -p 3000:3000 --env-file .env express-app-example",
    "lint": "eslint . --ext .ts,.js",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "node-env-resolver": "workspace:*",
    "@node-env-resolver/aws": "workspace:*",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "eslint": "^9.37.0",
    "@eslint/js": "^9.37.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "globals": "^16.4.0"
  }
}