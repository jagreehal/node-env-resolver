    "@types/aws-lambda": "^8.10.155",
    "@types/node": "^24.7.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "eslint": "^9.37.0",
    "@eslint/js": "^9.37.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "globals": "^16.4.0"
  }
}