{
  "name": "node-env-resolver-aws",
  "version": "7.0.0",
  "description": "AWS resolvers for node-env-resolver (Secrets Manager, SSM Parameter Store)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint src/",
    "test": "vitest run",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepublishOnly": "pnpm build && pnpm type-check"
  },
  "keywords": [
    "aws",
    "secrets-manager",
    "ssm",
    "parameter-store",
    "environment",
    "env",
    "config",
    "node-env-resolver"
  ],
  "author": {
    "name": "Jagvinder Singh Reehal",
    "email": "jag@jagreehal.com",
    "url": "https://jagreehal.com"
  },
  "license": "MIT",
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.929.0",
    "@aws-sdk/client-ssm": "^3.929.0"
  },
  "peerDependencies": {
    "node-env-resolver": "^6.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "node-env-resolver": "workspace:*",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  }
}