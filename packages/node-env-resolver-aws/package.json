{
  "name": "node-env-resolver-aws",
  "version": "10.0.0",
  "description": "AWS resolvers for node-env-resolver (Secrets Manager, SSM Parameter Store)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "NODE_ENV=production tsup",
    "build:dev": "tsup",
    "build:dts": "tsc --emitDeclarationOnly",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint src/",
    "test": "vitest run",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepublishOnly": "pnpm build && pnpm type-check"
  },
  "keywords": [
    "aws",
    "secrets-manager",
    "ssm",
    "parameter-store",
    "environment",
    "env",
    "config",
    "node-env-resolver"
  ],
  "author": {
    "name": "Jagvinder Singh Reehal",
    "email": "jag@jagreehal.com",
    "url": "https://jagreehal.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jagreehal/node-env-resolver.git",
    "directory": "packages/node-env-resolver-aws"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.952.0",
    "@aws-sdk/client-ssm": "^3.952.0"
  },
  "peerDependencies": {
    "node-env-resolver": "^6.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "node-env-resolver": "workspace:*",
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.2",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}